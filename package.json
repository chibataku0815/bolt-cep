    "@types/node": "^22.5.4",
    "typescript": "^5.6.2"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "adobe",
    "cep",
    "extension",
    "panel",
    "plugin",
    "typescript",
    "react",
    "bun",
    "premiere",
    "after-effects",
    "photoshop",
    "illustrator",
    "indesign"
  ]
