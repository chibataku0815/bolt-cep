{
  "name": "bolt-cep-bun",
  "version": "3.0.0",
  "description": "A Bun-powered boilerplate for Adobe CEP Extensions (React only)",
  "author": "hyperbrew (modified for Bun)",
  "license": "MIT",
  "homepage": "https://github.com/chibataku0815/bolt-cep",
  "repository": {
    "type": "git",
    "url": "https://github.com/chibataku0815/bolt-cep.git"
  },
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/build.ts --watch",
    "build": "bun run scripts/build.ts --production",
    "zxp": "bun run scripts/build.ts --production --zxp",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
